{
    "name": "quangvision/unit-conversions",
    "description": "Awesome unit conversion package in PHP",
    "keywords": [
        "quangvision",
        "unit-conversions"
    ],
    "homepage": "https://github.com/quangvision/unit-conversions",
    "license": "MIT",
    "authors": [
        {
            "name": "Quang Tran",
            "email": "quangvision@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpunit/phpunit": "^9.5",
        "spatie/ray": "^1.28",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Quangvision\\UnitConversions\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Quangvision\\UnitConversions\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
